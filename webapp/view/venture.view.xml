<mvc:View
	controllerName="sierra.pra.PRA_Custom.controller.venture"
	height="100%"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:core="sap.ui.core"
	xmlns:layout="sap.ui.layout"
	xmlns="sap.m"
	xmlns:f="sap.f"
	xmlns:fb="sap.ui.comp.filterbar"
	xmlns:vm="sap.ui.comp.variants">
	<f:DynamicPage id="dynamicPageId" headerExpanded="true">
		<!-- DynamicPage Title -->
		<f:title>
			<f:DynamicPageTitle>
				<f:heading>
					<vm:VariantManagement standardItemText="Venture">
					</vm:VariantManagement>
				</f:heading>
				<f:actions>
					<ToolbarSpacer/>
					<Button text="FILTER" icon="sap-icon://filter" type="Transparent" press="onPressFilter"/>
					<Button text="ADD" icon="sap-icon://create-form" type="Transparent" press="onDoiHeaderAddPress"/>
					<Button text="EDIT" icon="sap-icon://write-new-document" type="Transparent" press="onToggleFooter"/>
					<Button text="DELETE" icon="sap-icon://delete" type="Transparent"/>
					<Button text="CANCEL" icon="sap-icon://reset" type="Transparent"/>
				</f:actions>
			</f:DynamicPageTitle>
		</f:title>
		<!-- DynamicPage Header -->
		<f:header>
			<f:DynamicPageHeader pinnable="true">
				<f:content>
					<Table
						id="idProductsTable"
						inset="false"
						items="{ path: 'oModel>/Venture' }"
						selectionChange="SelectionChge"
						growing="true"
						growingThreshold="10">
						<columns>
							<Column minScreenWidth="Tablet" demandPopin="true" hAlign="Center"><Text text="Joint Venture"/></Column>
							<Column minScreenWidth="Tablet" demandPopin="true" hAlign="Center"><Text text="Venture Name"/></Column>
							<Column minScreenWidth="Tablet" demandPopin="true" hAlign="Center"><Text text="DOI"/></Column>
							<Column minScreenWidth="Tablet" demandPopin="true" hAlign="Center"><Text text="Company Code"/></Column>
							<Column minScreenWidth="Tablet" demandPopin="true" hAlign="Center"><Text text="Venture Interest code"/></Column>
							<Column minScreenWidth="Tablet" demandPopin="true" hAlign="Center"><Text text="Description"/></Column>
							<Column minScreenWidth="Tablet" demandPopin="true" hAlign="Center"><Text text="Venture status"/></Column>
							<Column minScreenWidth="Tablet" demandPopin="true" hAlign="Center"><Text text="Status effective Date"/></Column>
							<Column minScreenWidth="Tablet" demandPopin="true" hAlign="Center"><Text text="End Date"/></Column>
							<Column minScreenWidth="Tablet" demandPopin="true" hAlign="Center"><Text text="Country Key"/></Column>
							<Column minScreenWidth="Tablet" demandPopin="true" hAlign="Center"><Text text="Onshore/Offshore codes"/></Column>
							<Column minScreenWidth="Tablet" demandPopin="true" hAlign="Center"><Text text="Land Venture Number"/></Column>
						</columns>
						<items>
							<ColumnListItem type="Active" press="onSelectionChge" vAlign="Middle">
								<cells>
									<Text text="{oModel>Vname}"/>
									<Text text="{oModel>VnameNm}"/>
									<Text text="{oModel>OiuTimestamp}"/>
									<Text text="{oModel>Bukrs}"/>
									<Text text="{oModel>VnameIntCd}"/>
									<Text text="{oModel>Shtxt}"/>
									<Text text="{oModel>StsCode}"/>
									<Text text="{oModel>StsEffDt}"/>
									<Text text="{oModel>StsEffDt}"/>
									<Text text="{oModel>Country}"/>
									<Text text="{oModel>OnsOfsCd}"/>
									<Text text="{oModel>LpnNo}"/>
								</cells>
							</ColumnListItem>
						</items>
					</Table>
				</f:content>
			</f:DynamicPageHeader>
		</f:header>
		<!-- DynamicPage Content -->
		<f:content>
			<IconTabBar id="iconTabBarSix" class="sapUiResponsiveContentPadding"> 
				<items>
					<IconTabFilter icon="sap-icon://hint" iconColor="Positive" design="Horizontal" text="Attributes" key="Okpk" >
						<ToolbarSpacer/>
						<Button text="FILTER" icon="sap-icon://filter" type="Transparent" press="onPressBAFilter"/>
						<Button text="ADD" icon="sap-icon://create-form" type="Transparent" press="onDoiHeaderAddPress"/>
						<Table id="idProductsTable2" inset="false" items="{ path: 'oModel>/Venture' }" selectionChange="onSelectRequestProgramList" growing="true" growingThreshold="10">
							<columns>
								<Column minScreenWidth="Tablet" demandPopin="true"><Text text="Geo Loc"/></Column>
								<Column minScreenWidth="Tablet" demandPopin="true"><Text text="2Geo Loc"/></Column>
								<Column minScreenWidth="Tablet" demandPopin="true"><Text text="Acquired Date"/></Column>
								<Column minScreenWidth="Tablet" demandPopin="true"><Text text="Venture Usage Type"/></Column>
								<Column minScreenWidth="Tablet" demandPopin="true"><Text text="Old Lease ID"/></Column>
								<Column minScreenWidth="Tablet" demandPopin="true"><Text text="Unit Venture Flag"/></Column>
								<Column minScreenWidth="Tablet" demandPopin="true"><Text text="DOI by Track Flag"/></Column>
								<Column minScreenWidth="Tablet" demandPopin="true"><Text text="Drill Fund Flag"/></Column>
							</columns>
							<items>
								<ColumnListItem vAlign="Middle">
									<cells>
										<Text text="{oModel>PriGeoLoc}"/>
										<Text text="{oModel>SecGeoLoc}"/>
										<Text text="{oModel>AcqDt}"/>
										<Text text="{oModel>TypeCd}"/>
										<Text text="{oModel>OldVnameNo}"/>
										<Text text="{oModel>UnitFl}"/>
										<Text text="{oModel>DoiByTrFl}"/>
										<Text text="{oModel>DrlFundFl}"/>
									</cells>
								</ColumnListItem>
							</items>
						</Table>
					</IconTabFilter>
					<IconTabFilter icon="sap-icon://hint" iconColor="Positive" design="Horizontal" text="Unit/Tract" key="Okpk" >
						<!-- Table commented out, see original for details -->
					</IconTabFilter>
				</items>
			</IconTabBar>
		</f:content>
		<!-- DynamicPage Footer -->
		<f:footer>
			<OverflowToolbar>
				<ToolbarSpacer/>
				<Button type="Accept" text="Accept"/>
				<Button type="Reject" text="Reject"/>
			</OverflowToolbar>
		</f:footer>
	</f:DynamicPage>
</mvc:View>