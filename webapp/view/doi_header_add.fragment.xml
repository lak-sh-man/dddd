<core:FragmentDefinition class="sapUiSizeCompact" xmlns="sap.m" xmlns:table="sap.ui.table" xmlns:core="sap.ui.core" xmlns:u="sap.ui.unified"
	xmlns:f="sap.ui.layout.form" xmlns:layout="sap.ui.layout">
	<Dialog id="ErrorLogPopover" title="Create Venture">
		<beginButton>
			<Button text="Save" press=".onCloseErrorLogPopover"/>
		</beginButton>
		<endButton>
			<Button text="Cancel" press=".onCloseErrorLogPopover"/>
		</endButton>
		<VBox class="sapUiSmallMargin">
			<f:SimpleForm editable="true" layout="ColumnLayout" columnsM="2" columnsL="2" labelSpanXL="1" labelSpanL="2" labelSpanM="2" labelSpanS="2"
				adjustLabelSpan="false" emptySpanXL="0" emptySpanL="0" emptySpanM="0" emptySpanS="0" minWidth="500">
				<!-- Form fields for DOI header creation are commented out for future implementation -->
			</f:SimpleForm>
		</VBox>
	</Dialog>
</core:FragmentDefinition>